{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "treatUndefinedAsUnspecified": 0,
        "consistent-return": 0
    },
    "ignorePatterns": ["node_modules/", "/sequelize", "/__tests__"]

}
